<?xml version="1.0" encoding="utf-8" ?>
<configuration>
<!-- 产品名称:云通信短信API产品,开发者无需替换-->
<product>Dysmsapi</product>
<!--产品域名,开发者无需替换-->
<domain>dysmsapi.aliyuncs.com</domain>
<!--链接超时时间-->
<DEFAULT_CONNECT_TIMEOUT>10000</DEFAULT_CONNECT_TIMEOUT>
<!-- 读取超时时间-->
<DEFAULT_READ_TIMEOUT>10000</DEFAULT_READ_TIMEOUT>
<!-- 地区ID 此为阿里云短信平台制定规范-->
<REGION_ID>cn-hangzhou</REGION_ID>
<!--结束命名规范 此为阿里云短信平台制定规范-->
<END_POINT_NAME>cn-hangzhou</END_POINT_NAME>
<SYSTEM_CODE>sendMessage</SYSTEM_CODE>
</configuration>
